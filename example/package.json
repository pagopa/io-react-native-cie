{
  "name": "@pagopa/io-react-native-cie-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "npx tsc --noEmit",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --scheme IoReactNativeCieExample --verbose --mode Debug --extra-params \"-sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\""
  },
  "dependencies": {
    "@pagopa/io-app-design-system": "5.11.5",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-navigation/elements": "^2.4.1",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.12",
    "dayjs": "^1.11.13",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-circular-progress": "^1.4.1",
    "react-native-easing-gradient": "^1.1.1",
    "react-native-gesture-handler": "^2.29.0",
    "react-native-haptic-feedback": "^2.3.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.19.3",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "^15.14.0",
    "react-native-webview": "^13.16.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.5",
    "@react-native/eslint-config": "0.81.5",
    "@react-native/metro-config": "0.81.5",
    "@react-native/typescript-config": "0.81.5",
    "@types/react": "^19.1.0",
    "react-native-builder-bob": "^0.40.6"
  },
  "engines": {
    "node": ">=18"
  }
}
